<?xml version="1.0" encoding="iso-8859-1"?>

<PropertyList>

  <default>

    <menu n="11">

      <label>Mirage 2000</label>
      <enabled type="bool">true</enabled>

      <item n="1">
        <label>Configuration</label>
        <name>config</name>
        <binding>
          <command>dialog-show</command>
          <dialog-name>config</dialog-name>
        </binding>
      </item>
      
      <item n="2">
        <label>Flight Mode</label>
        <name>FlightMode</name>
        <binding>
          <command>dialog-show</command>
          <dialog-name>flightmode</dialog-name>
        </binding>
      </item>
      
      <item n="3">
        <label>Link16</label>
        <name>AboutMirage</name>
        <binding>
          <command>dialog-show</command>
          <dialog-name>link16</dialog-name>
        </binding>
      </item>
      
      
      <item n="4">
        <label>Lights</label>
        <name>lights</name>
        <binding>
          <command>dialog-show</command>
          <dialog-name>lightning</dialog-name>
        </binding>
      </item>

      <item n="5">
        <label>Select Livery</label>
        <binding>
          <command>nasal</command>
          <script>aircraft.livery.dialog.toggle()</script>
        </binding>
      </item>

      <item n="6">
        <label>Select Logos</label>
        <binding>
          <command>nasal</command>
          <script>mirage2000.logo_dialog.toggle()</script>
        </binding>
      </item>

      <item n="7">
        <label>Options</label>
        <name>options</name>
        <binding>
          <command>dialog-show</command>
          <dialog-name>options</dialog-name>
        </binding>
      </item>

      <item n="8">
        <label>Radar</label>
        <name>radar</name>
        <binding>
          <command>dialog-show</command>
          <dialog-name>radar</dialog-name>
        </binding>
      </item>
