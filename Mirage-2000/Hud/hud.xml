  <!-- ~~~~~~~~~~~~~~~~~~ stall alert -->
  <label>
    <name>StallAlert</name>
    <x>-35</x>
    <y>-45</y>
    <width>15</width>
    <height>6</height>
    <condition>
      <equals>
        <property>/sim/alarms/stall-warning</property>
        <value>2</value>
      </equals>
    </condition>
    <format>STALL ALERT !</format>
    <blinking>
      <interval>0.1</interval>
    </blinking>
    <halign>left</halign>
  </label>

  <!-- ~~~~~~~~~~~~~~~~~~ stall warning -->
  <label>
    <name>StallWarning</name>
    <x>-40</x>
    <y>-45</y>
    <width>15</width>
    <height>6</height>
    <condition>
      <equals>
        <property>/sim/alarms/stall-warning</property>
        <value>1</value>
      </equals>
    </condition>
    <format>STALL WARNING !</format>
    <blinking>
      <interval>0.25</interval>
    </blinking>
    <halign>left</halign>
  </label>

  <!-- ~~~~~~~~~~~~~~~~~~ altitude to low -->
  <label>
    <name>elevationWarn</name>
    <x>0</x>
    <y>-35</y>
    <condition>
      <and>
        <equals>
          <property>/controls/gear/gear-down</property>
          <value>false</value>
        </equals>
        <less-than>
          <property>/position/altitude-agl-ft</property>
          <value>300</value>
        </less-than>
        <less-than>
          <property>/instrumentation/vertical-speed-indicator/indicated-speed-kts</property>
          <value>0</value>
        </less-than>
      </and>
    </condition>
    <halign>center</halign>
    <format>PULL UP ! %5.0fft</format>
    <input>
      <property>/position/altitude-agl-ft</property>
    </input>
    <blinking>
      <interval>0.25</interval>
    </blinking>
  </label>

